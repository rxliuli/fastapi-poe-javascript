{
  "name": "fastapi-poe",
  "version": "0.1.4",
  "type": "module",
  "bin": {
    "poe": "./bin.js"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "url": "https://github.com/rxliuli/fastapi-poe-javascript",
    "type": "git"
  },
  "files": [
    "dist",
    "packages",
    "bin.js"
  ],
  "scripts": {
    "init": "pnpm build",
    "build": "vite build",
    "dev": "pnpm build --watch",
    "start": "vite-node src/bin.ts",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@liuli-util/test": "^3.8.0",
    "@liuli-util/vite-plugin-node": "^0.9.0",
    "@types/node": "^22.4.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vite-node": "^2.0.5",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "hono": "^4.5.6"
  },
  "pnpm": {
    "overrides": {
      "fastapi-poe": "file:."
    }
  }
}
